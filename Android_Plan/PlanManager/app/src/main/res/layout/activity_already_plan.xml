<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.root.planmanager.Already_plan">

    <TextView
        android:id="@+id/t_alreadyplan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="30px"
        android:layout_marginLeft="50px"

        android:text="已处理事项"
        />
    <ListView
        android:id="@+id/list_alreadyplan"
        android:layout_width="368dp"
        android:layout_height="540dp"
        android:entries="@array/names"
        app:layout_constraintTop_toBottomOf="@id/t_alreadyplan"
        app:layout_constraintLeft_toLeftOf="@id/t_alreadyplan"


        />



</android.support.constraint.ConstraintLayout>
