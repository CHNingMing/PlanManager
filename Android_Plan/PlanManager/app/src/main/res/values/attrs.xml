<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 自定义属性 -->
    <!-- 指定控件属性 -->
    <declare-styleable name="item_layout_itemsOpt">
        <attr name="plan_id" format="integer" />
    </declare-styleable>
    <declare-styleable name="ItemImageItemsOpt">
        <attr name="srcCompat" />
